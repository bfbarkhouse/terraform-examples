import tfplan-functions as plan
allPublicIPs = plan.find_resources("azurerm_public_ip")
allowedPublicIPAllocMethod = "Dynamic"
violatingPublicIPs = plan.filter_attribute_is_not_value(allPublicIPs, "allocation_method", allowedPublicIPAllocMethod, true)

violations = length(violatingPublicIPs["messages"])
main = rule {
    violations is 0
}

